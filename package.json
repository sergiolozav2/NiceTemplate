{
  "name": "nicetemplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test": "npx playwright test",
    "test:report": "pnpm exec playwright show-report",
    "lint": "eslint . && prettier --check .",
    "lint:fix": "eslint --fix --quiet .; prettier --list-different --write .",
    "docs:gen": "pnpm exec typedoc --out docs --plugin typedoc-theme-hierarchy --theme hierarchy --entryPointStrategy expand ./src",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "husky:lint-staged": "lint-staged",
    "husky:commitlint": "npx --no-install commitlint --edit"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.9.0",
    "@playwright/test": "^1.47.2",
    "@types/node": "^22.7.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-check-file": "^2.8.0",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.13",
    "typedoc": "^0.26.7",
    "typedoc-theme-hierarchy": "^5.0.0",
    "typescript": "~5.5.4",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.1"
  }
}
